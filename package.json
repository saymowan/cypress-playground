{
  "name": "cypress-playground",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cypress:open-prod": "cypress open --config-file=production.config.js",
    "cypress:open-stg": "cypress open --config-file=stage.config.js",
    "cypress:chrome-prod": "cypress run --browser chrome --config-file=production.config.js",
    "cypress:chrome-smoke-prod": "cypress run --browser chrome --config-file=production.config.js -- --env grep=@smoke",
    "cypress:chrome-smoke-stg": "cypress run --browser chrome --config-file=stage.config.js ",
    "cypress:firefox": "cypress run --browser firefox",
    "cypress:webkit": "cypress run --browser webkit",
    "cypress:edge": "cypress run --browser edge"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^10.10.0"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "minimist": "^1.2.7"
  }
}
